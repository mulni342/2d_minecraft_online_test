<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%- name%> - Menu</title>
    <link rel="stylesheet" href="/css/menu.css">
    <script>
        var name = <%- JSON.stringify(name)%>;
        var login_token = <%- JSON.stringify(login_token)%>;
        var worlds = <%- JSON.stringify(worlds)%>;
    </script>
    <script src="/socket.io/socket.io.js"></script>
</head>
<body>
    <header>
        <div class="user_cont">
            <span><%- name%></span>
        </div>
        <div class="join_server_cont">
            <input type="text" maxlength="5" placeholder="SERVER CODE">
            <button>JOIN</button>
        </div>
    </header>
    <main>
        <div class="world_title">Your Worlds:</div>
        <div class="worlds_cont"></div>
    </main>
    <script src="/js/menu.js"></script>
</body>
</html>